{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "jsx": "react",
    "baseUrl": "./src", //typescript 根路径（绝对路径的根节点）
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node", //可以使用无“/”分割的绝对路径
    "strictNullChecks": true
  },
  "include": ["src","node_modules"],
  // "exclude": ["node_modules"]
}
